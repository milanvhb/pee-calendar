<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox minHeight="600.0" minWidth="1360.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.PatientCalendar">
   <children>
      <VBox HBox.hgrow="ALWAYS">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="40.0" style="-fx-background-color: #2B2B2B;" VBox.vgrow="NEVER">
               <children>
                  <HBox HBox.hgrow="ALWAYS">
                     <children>
                        <Label style="-fx-font-size: 26; -fx-font-weight: bold; -fx-text-fill: white;" text="Consultation Calendar" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" spacing="20.0">
                     <children>
                        <Label fx:id="userLabel" style="-fx-text-fill: white; -fx-font-size: 14; -fx-font-weight: bold;" text="Label" />
                        <Hyperlink fx:id="logoutLink" text="Logout" />
                     </children>
                     <padding>
                        <Insets right="20.0" />
                     </padding>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="5.0" left="20.0" top="5.0" />
               </padding>
            </HBox>
            <HBox fx:id="mainContentPane" alignment="CENTER" VBox.vgrow="ALWAYS">
               <padding>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_RIGHT" VBox.vgrow="NEVER">
               <children>
                  <Button fx:id="saveButton" defaultButton="true" minHeight="35.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="120.0" text="Save" />
               </children>
               <padding>
                  <Insets bottom="10.0" right="20.0" top="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</HBox>
